@model IEnumerable<E_shop.Models.Items>
@{
    ViewBag.Title = "Home Page";




}


<head>
    <link href="~/Content/Index.css" rel="stylesheet" />
</head>
<br />
<br />

<body class="font">
    <div class="category-border-wrap col-sm-3">
        <div onclick="location.href='https://localhost:44368/Home/AddImage';" class="category">
            COMPUTER

        </div>
    </div>
    <div class="category-border-wrap col-sm-3">
        <div onclick="location.href='https://localhost:44368/Home/AddImage';" class="category">
            MOBIL
        </div>
    </div>
    <div class="category-border-wrap col-sm-3">
        <div onclick="location.href='https://localhost:44368/Home/AddImage';" class="category">
            TV & LYD
        </div>
    </div>
    <div class="category-border-wrap col-sm-3">
        <div onclick="location.href='https://localhost:44368/Home/AddImage';" class="category">
            GAMING
        </div>
    </div>
    <br /><br /><br /><br />
</body>
<table class="table table-striped">
    <tbody>
        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="3" style="color:red">
                    No Match Any Document
                </td>
            </tr>
        }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                   
                    <td> <img src='@item.ImagePath' style=" width: 100px; height: 100px;" /></td>
                    <td>@item.ItemName</td>
                    <td>@item.Description</td>
                    @{
               
                    }
                    <td>      @item.ItemPrice,00 kr. </td>

                    <td>
                       
                    </td>


                </tr>

            }
        }
    </tbody>
</table>


@if (Model.Count() == 0)
{
    <tr>
        <td>
            Intet matchede dit søge resultat
        </td>
    </tr>
}